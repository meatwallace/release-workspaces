    "test": "jest"
  },
  "dependencies": {
    "conventional-commits-filter": "2.0.1",
    "conventional-commits-parser": "3.0.1",
    "execa": "2.0.3",
    "is-ci": "2.0.0",
    "simple-git": "1.118.0",
    "tiny-invariant": "1.0.5",
    "tiny-warning": "1.0.3"
  },
  "devDependencies": {
    "@berry/pnpify": "0.0.6",
    "@commitlint/cli": "8.0.0",
    "@commitlint/prompt-cli": "8.0.0",
    "@meatwallace/commitlint-config": "1.2.0",
    "@meatwallace/jest-config-base": "1.3.0",
    "@meatwallace/prettier-config": "1.2.0",
    "@types/chalk": "2.2.0",
    "@types/execa": "0.9.0",
    "@types/is-ci": "2.0.0",
    "@types/jest": "24.0.15",
    "babel-plugin-transform-async-to-promises": "0.8.12",
    "chalk": "2.4.2",
    "husky": "3.0.0",
    "jest": "24.8.0",
    "jest-watch-typeahead": "0.3.1",
    "lint-staged": "9.1.0",
    "nodemon": "1.19.1",
    "prettier": "1.18.2",
    "pretty-quick": "1.11.1",
    "ts-jest": "24.0.2",
    "tsdx": "0.7.2",
    "tslib": "1.10.0",
    "typescript": "3.5.3"
  },
  "files": [
    "CHANGELOG.md",
    "dist",
    "package.json",
    "README.md",
    "yarn.lock"
  ],
  "keywords": [
    "yarn",
    "workspaces",
    "npm",
    "publish",
    "release",
    "semantic-versioning",
    "cli"
  ]
